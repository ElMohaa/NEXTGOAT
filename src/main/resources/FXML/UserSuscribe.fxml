<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorpane" minHeight="615.0" minWidth="100.0" prefHeight="161.0" prefWidth="315.0" stylesheets="@../CSS/suscribe.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.next_goat.Controllers.UserSuscribeController">
   <children>
      <!-- Botón para regresar a la ventana de Login -->
      <Button fx:id="backToLoginButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBackToLoginButtonAction" text="Login" />
      <!-- Botón para hacer el registro (Sign Up) -->
      <Button fx:id="signUpButton" layoutX="106.0" layoutY="501.0" mnemonicParsing="false" onAction="#handleSignUpButtonAction" prefHeight="36.0" prefWidth="94.0" text="SIGN UP" />
      <!-- Campos de texto para el registro -->
      <TextField fx:id="nameField" layoutX="29.0" layoutY="148.0" prefHeight="26.0" prefWidth="124.0" promptText="Name" />
      <TextField fx:id="surnameField" layoutX="170.0" layoutY="148.0" prefHeight="26.0" prefWidth="110.0" promptText="Surnames" />
      <TextField fx:id="usernameField" layoutX="27.0" layoutY="316.0" promptText="Username" />
      <TextField fx:id="passwordField" layoutX="27.0" layoutY="348.0" promptText="Password" />
      <TextField fx:id="emailField" layoutX="27.0" layoutY="412.0" promptText="Email" />
      <TextField fx:id="phoneField" layoutX="28.0" layoutY="446.0" promptText="Telephone Number" />
      <TextField fx:id="addressField" layoutX="30.0" layoutY="222.0" prefHeight="26.0" prefWidth="102.0" promptText="Address" />
      <DatePicker fx:id="datePicker" layoutX="30.0" layoutY="261.0" promptText="Date of Birth" />
      <Button fx:id="botnSing" layoutX="76.0" layoutY="14.0" mnemonicParsing="false" text="Sing Up" />
      <TextField fx:id="dniField" layoutX="30.0" layoutY="189.0" promptText="DNI or NIE" />
      <ImageView fitHeight="115.0" fitWidth="110.0" layoutX="98.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../IMAGENS/LOGO.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
